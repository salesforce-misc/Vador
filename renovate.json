{
  "extends" : [
    "config:base"
  ],
  "automerge" : true,
  "commitBodyTable" : true,
  "packageRules" : [
    {
      "matchPackagePatterns" : [
        "*"
      ],
      "matchUpdateTypes" : [
        "major",
        "minor",
        "patch"
      ],
      "groupName" : "all dependencies",
      "groupSlug" : "all"
    },
    {
      "matchPackageNames": [
        "de.cronn:reflection-util"
      ],
      "enabled": false
    }
  ]
}
